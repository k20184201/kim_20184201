{
    "env": {
        "es6": true,
        "commonjs": true,
        "node": true
    },
    "extends": ["eslint:recommended","prettier"],
    "globals":{
        "Automics":"readonly",
    "SharedArrayBuffer":"readonly"
},
    "parserOptions": {
        "ecmaVersion": 2018
    },
    
    "rules": {
        "no-unused-vars":"warn",
        "no-console":"off"
    }
}
